{
  "name": "mars-server",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "siyuan.cao <ll900618@163.com>",
  "homepage": "https://github.com/CaoLeilei/Mars#readme",
  "license": "ISC",
  "main": "lib/mars-server.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ]
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "orm": "npx mikro-orm",
    "orm:create": "npx mikro-orm migration:create",
    "orm:up": "npx mikro-orm migration:up",
    "orm:down": "npx mikro-orm migration:down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@mikro-orm/better-sqlite": "^6.2.5",
    "@mikro-orm/core": "^6.2.5",
    "@mikro-orm/migrations": "6.2.5",
    "@mikro-orm/mysql": "^6.2.5",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/reflection": "^6.2.5",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "argon2": "^0.40.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "helper-fns": "^2.7.0",
    "isomorphic-dompurify": "^2.10.0",
    "joi": "^17.13.1",
    "nestjs-i18n": "^10.4.5",
    "nestjs-pino": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-http": "^10.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.2.5",
    "@mikro-orm/seeder": "^6.2.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@rubiin/tsconfig": "^1.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/common/database/mikro-orm-cli.config.ts",
      "./dist/common/database/mikro-orm-cli.config.js"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmmirror.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoLeilei/Mars.git"
  },
  "bugs": {
    "url": "https://github.com/CaoLeilei/Mars/issues"
  }
}
